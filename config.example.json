{
  "agent": {
    "workspace": "~/.myclaw/workspace",
    "model": "claude-sonnet-4-5-20250929",
    "maxTokens": 8192,
    "temperature": 0.7,
    "maxToolIterations": 20,
    "errorGuard": {
      "enabled": true,
      "threshold": 2,
      "markers": []
    }
  },
  "provider": {
    "type": "anthropic",
    "apiKey": "${ANTHROPIC_API_KEY}",
    "baseUrl": ""
  },
  "tools": {
    "braveApiKey": "${BRAVE_API_KEY}",
    "execTimeout": 60,
    "restrictToWorkspace": true
  },
  "channels": {
    "telegram": {
      "enabled": false,
      "token": "${TELEGRAM_BOT_TOKEN}",
      "allowFrom": [],
      "proxy": ""
    },
    "feishu": {
      "enabled": false,
      "appId": "",
      "appSecret": "",
      "verificationToken": "",
      "encryptKey": "",
      "port": 8080,
      "allowFrom": []
    },
    "wecom": {
      "enabled": false,
      "token": "",
      "encodingAESKey": "",
      "receiveId": "",
      "port": 8081,
      "allowFrom": []
    }
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 18790
  }
}
