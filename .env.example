# aevitas environment variables for docker-compose
# Copy this file to .env and fill in your values:
#   cp .env.example .env

# Required: AI provider API key
AEVITAS_API_KEY=sk-ant-...

# Optional: Telegram bot token
AEVITAS_TELEGRAM_TOKEN=

# Optional: Feishu bot credentials
AEVITAS_FEISHU_APP_ID=
AEVITAS_FEISHU_APP_SECRET=

# Optional: WeCom intelligent bot credentials
AEVITAS_WECOM_TOKEN=
AEVITAS_WECOM_ENCODING_AES_KEY=
AEVITAS_WECOM_RECEIVE_ID=
